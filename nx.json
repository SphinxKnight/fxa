{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "fxa",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "build-l10n",
          "build-l10n-test",
          "build-storybook",
          "compile",
          "lint",
          "prebuild",
          "test",
          "test-unit",
          "test-integration",
          "test-e2e",
          "test-integration-auth-server-p1",
          "test-integration-auth-server-p2",
          "test-integration-frontend",
          "test-integration-auth-scripts",
          "test-integration-auth-oauth",
          "test-integration-auth-remote",
          "test-integration-auth-local",
          "test-integration-api",
          "test-integration-shared"
        ],
        "accessToken": "ODI2MGMxMDYtZjI1OS00MWM4LTg4YjEtOTMzNmRlNzEwNzU4fHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "compile": {
      "dependsOn": [
        "^compile"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "prebuild": {
      "dependsOn": [
        "^prebuild"
      ],
      "inputs": [
        "production",
        "^production"
      ],
      "outputs": [
        "{projectRoot}/build",
        "{projectRoot}/dist",
        "{projectRoot}/public/locales",
        "{projectRoot}/public/legal-docs",
        "{workspaceRoot}/packages/fxa-profile-server/var/public",
        "{workspaceRoot}/packages/fxa-auth-server/vendor/ejs.js",
        "{workspaceRoot}/packages/fxa-content-server/app/styles/main.css",
        "{workspaceRoot}/packages/fxa-content-server/server/config/local.json"
      ]
    },
    "build": {
      "dependsOn": [
        "^build",
        "prebuild"
      ],
      "inputs": [
        "production",
        "^production"
      ],
      "outputs": [
        "{projectRoot}/build",
        "{projectRoot}/dist",
        "{projectRoot}/public/locales",
        "{projectRoot}/public/legal-docs"
      ]
    },
    "build-storybook": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "production",
        "^production"
      ],
      "outputs": [
        "{project-root}/storybook-static"
      ]
    },
    "lint": {
      "inputs": [
        "production",
        "^production"
      ],
      "outputs": [
        "{projectRoot}/.eslintcache"
      ]
    },
    "test": {
      "inputs": [
        "production",
        "^production"
      ],
      "dependsOn": [
        "test-unit",
        "test-integration",
        "test-e2e"
      ],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-unit": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "production",
        "^production"
      ],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration": {
      "dependsOn": [
        "build",
        "build-l10n-test"
      ],
      "inputs": [
        "production",
        "^production"
      ],
