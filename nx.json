{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "fxa",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "build-l10n",
          "build-l10n-test",
          "build-storybook",
          "compile",
          "lint",
          "prebuild",
          "test",
          "test-unit",
          "test-integration",
          "test-e2e",
          "test-integration-auth-server-p1",
          "test-integration-auth-server-p2",
          "test-integration-frontend",
          "test-integration-auth-scripts",
          "test-integration-auth-oauth",
          "test-integration-auth-remote",
          "test-integration-auth-local",
          "test-integration-api",
          "test-integration-shared"
        ]
      }
    }
  },
  "targetDefaults": {
    "compile": {
      "dependsOn": ["^compile"],
      "inputs": ["production", "^production"]
    },
    "prebuild": {
      "dependsOn": ["^prebuild"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/build",
        "{projectRoot}/dist",
        "{projectRoot}/locales",
        "{projectRoot}/public/locales",
        "{projectRoot}/public/legal-docs",
        "{projectRoot}/var/public",
        "{projectRoot}/vendor/ejs.js",
        "{projectRoot}/app/styles/main.css",
        "{projectRoot}/server/config/local.json"
      ]
    },
    "build": {
      "dependsOn": ["^build", "prebuild"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/build",
        "{projectRoot}/dist",
        "{projectRoot}/public/locales",
        "{projectRoot}/public/legal-docs",
        "{projectRoot}/test/react.ftl"
      ]
    },
    "build-storybook": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": ["{project-root}/storybook-static"]
    },
    "lint": {
      "inputs": ["production", "^production"],
      "outputs": ["{projectRoot}/.eslintcache"]
    },
    "test": {
      "inputs": ["production", "^production"],
      "dependsOn": ["test-unit", "test-integration", "test-e2e"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-unit": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration": {
      "dependsOn": ["build", "build-l10n-test"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-e2e": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration-auth-server-p1": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration-auth-server-p2": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration-frontend": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration-auth-scripts": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration-auth-oauth": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration-auth-remote": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration-auth-local": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration-api": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "test-integration-shared": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": [
        "{projectRoot}/coverage",
        "{projectRoot}/.nyc_output",
        "{projectRoot}/test-results.xml"
      ]
    },
    "start": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": []
    },
    "storybook": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": []
    },
    "restart": {
      "dependsOn": ["build"],
      "inputs": ["production", "^production"],
      "outputs": []
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/package.json"],
    "production": ["default"]
  },
  "cli": {
    "packageManager": "yarn"
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "libs"
  }
}
